    "outDir": "../../../public/administration",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "target": "es2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "allowJs": true,
    "sourceMap": true,
